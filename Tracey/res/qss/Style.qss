}

HighlightingWidget {
  background: %1;
  border: 1px solid %4;
}

MarkWidget {
  background: %1;
  border: 1px solid %4;
}

SearchProgressDialog {
  border: 1px solid %4;
}

AboutQtDialog {
  border: 1px solid %4;
}

QFrame#CentralWidget {
  background: #ffffff;
}
QFrame#TraceViewerMargin {
  background: #f8f8f8; 
}
QFrame#TraceViewerViewport {
  border: 1px solid %3;
}

QLabel#FontDialogSampleText {
  background: #ffffff; 
  border:1px solid %3;
}